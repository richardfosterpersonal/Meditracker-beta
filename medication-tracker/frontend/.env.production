# API Configuration
REACT_APP_API_URL=https://api.medication-tracker.com
REACT_APP_API_VERSION=v1

# Security
REACT_APP_SENTRY_DSN=your-sentry-dsn
REACT_APP_MIXPANEL_PROD_TOKEN=your-mixpanel-token

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_ERROR_MONITORING=true
REACT_APP_ENABLE_PERFORMANCE_MONITORING=true

# PWA Configuration
REACT_APP_VAPID_PUBLIC_KEY=your-vapid-public-key

# CDN Configuration
REACT_APP_CDN_URL=https://cdn.medication-tracker.com

# Cache Configuration
REACT_APP_CACHE_VERSION=1
REACT_APP_CACHE_DURATION=86400

# Rate Limiting
REACT_APP_RATE_LIMIT_REQUESTS=100
REACT_APP_RATE_LIMIT_WINDOW=60

# Backup Configuration
REACT_APP_BACKUP_RETENTION_DAYS=30
REACT_APP_MAX_BACKUPS=10

# Monitoring Configuration
REACT_APP_LOG_LEVEL=error
REACT_APP_PERFORMANCE_SAMPLE_RATE=0.1
