{
  "name": "medication-tracker-backend",
  "version": "1.0.0",
  "description": "Backend for the medication tracker application",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "start": "node --experimental-specifier-resolution=node dist/index.js",
    "dev": "node --loader ts-node/esm --experimental-specifier-resolution=node src/index.ts",
    "migrate": "prisma migrate dev",
    "migrate:prod": "prisma migrate deploy",
    "generate": "prisma generate",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --reporters='default' --reporters='jest-junit'",
    "build": "npm run fix:imports && tsc",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "validate": "tsc --noEmit",
    "seed": "node --loader ts-node/esm --experimental-specifier-resolution=node prisma/seed.ts",
    "fix:imports": "node ../scripts/fix-es-modules.js",
    "fix:all": "npm run fix:imports && npm run format && npm run lint"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.450.0",
    "@prisma/client": "^5.7.0",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.9.0",
    "@types/pg": "^8.10.9",
    "bcrypt": "^5.1.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "glob": "^11.0.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "prisma": "^5.7.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@jest/types": "^29.6.3",
    "@types/bcrypt": "^5.0.2",
    "@types/ioredis": "^5.0.0",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1"
  }
}
